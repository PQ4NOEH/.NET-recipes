<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <PackageProfile>Cloud</PackageProfile>
    <PackageSolutionConfiguration>Release</PackageSolutionConfiguration>
    <PackageEnableRemoteDesktop>False</PackageEnableRemoteDesktop>
    <WebRoleHttpsSkipLaunchInBrowser>
    </WebRoleHttpsSkipLaunchInBrowser>
    <DeploymentProfilingMode>Sampling</DeploymentProfilingMode>
    <DeploymentEnableTierInteractionProfiling>False</DeploymentEnableTierInteractionProfiling>
    <ActivePublishProfile>heracles-staging</ActivePublishProfile>
    <RemoteDesktopMoreOptions>True</RemoteDesktopMoreOptions>
    <LocalProfile>Local</LocalProfile>
  </PropertyGroup>
</Project>